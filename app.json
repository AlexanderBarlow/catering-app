{
  "expo": {
    "name": "Catering Ops",
    "slug": "catering-app",
    "scheme": "cateringops",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF6F2"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abarlow17.cateringapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.abarlow17.cateringapp",
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app.png",
        "backgroundColor": "#FFF6F2"
      }
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": ["expo-router"],

    "extra": {
      "router": {},
      "eas": {
        "projectId": "a8352aa5-a6c8-4323-9e4d-e8b29e62baad"
      }
    },

    "runtimeVersion": {
      "policy": "appVersion"
    },

    "updates": {
      "url": "https://u.expo.dev/a8352aa5-a6c8-4323-9e4d-e8b29e62baad"
    }
  }
}
